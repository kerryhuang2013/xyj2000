#class login open;
#action {重新连线完毕} {
    login;
};

#alias {login} {
    #send {score};
};

#action {【%*】%* %*(%*)} {
    #var char_name %3;
    #var char_id @lower{%4};
    #class login kill;
    #send {hp};
    #class login kill;
    #delay {2} {
        show.init;        
    };
};

#class login close;
