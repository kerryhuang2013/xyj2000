#class watchdog open;
#al {wd.start} {
    #al wd_action #cr;
    #al wd_action %0;
    #delay {wd_delay} {
        wd_action;
    } {600};
};

#al {wd.stop} {
    #al wd_action #cr;
    #undelay {wd_delay};
};

#al {wd.kick} {
    #delay {wd_delay} {
        wd_action;
    } {600};
};
#class watchdog close;
