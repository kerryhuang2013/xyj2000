#nop ==路径计算==

#alias routecreate {
    #show Calculating route from @fullname{$CurrentPosition} to @fullname{$Destination} ...;
    #nop ----------定义变量----------;
    #var Route {};
    #var Route_list {};
    #var Route_list_from {};
    #var AreaOpen_list {};
    #var AreaOpen_list_from {};
    #list AreaOpen_list_prev create {$CurrentPosition};
    #list AreaClose_list create {$CurrentPosition};
    #list AreaAbbr_list find $CurrentPosition AreaId;
    #var Degree 1;
    #while {@numitems{Route}==0} {
        #nop //----------开始计算----------;
        #nop        #show -$Degree-;
        #math Degree {$Degree+1};
        #nop //----------生成当前度数的路径列表----------;
        #var AreaOpen_list {};
        #var AreaOpen_list_from {};
        #loop @numitems{AreaOpen_list_prev} 1 i {
            #var AreaId @ismember{@item{AreaOpen_list_prev;$i};AreaAbbr_list};
            #loop @item{RouteConnection_count;$AreaId} 1 j {
                #if {@ismember{@leadto{$AreaId;$j};AreaClose_list}==0} {
                    #list AreaOpen_list add {@leadto{$AreaId;$j}};
                    #list AreaOpen_list_from add {@item{AreaAbbr_list;$AreaId}};
                    #list AreaClose_list add @leadto{$AreaId;$j};
                };
            };
        };
        #nop        #show Degree: $Degree;
        #nop        #show AreaOpen_list: $AreaOpen_list[%*];
        #nop        #show AreaOpen_list_from: $AreaOpen_list_from[%*];
        #if {$Degree>100} {
            #show Error!!;
            #break;
        };
        #nop //----------搜索目的地----------;
        #if {@ismember{$Destination;AreaOpen_list}>0} {
            #nop //----------发现目的地----------;
            #nop            #show =$Degree=;
            #list Route create {$Destination};
            #var i @ismember{$Destination;AreaOpen_list};
            #var j @item{AreaOpen_list_from;$i};
            #while {$Degree>1} {
                #list Route add {$j};
                #var i @ismember{$j;Route_list};
                #var j @item{Route_list_from;$i};
                #math Degree {$Degree-1};
            };
            #nop            #list Route add {$CurrentPosition};
            #break;
        };
        #else {
            #nop //----------未发现目的地----------;
            #var AreaOpen_list_prev $AreaOpen_list;
            #loop @numitems{AreaOpen_list} 1 i {
                #list Route_list add {@item{AreaOpen_list;$i}};
                #list Route_list_from add {@item{AreaOpen_list_from;$i}};
            };
        };
    };
    #show {Creating route ...};
    #nop //----------生成路径列表----------;
    #var Route_list {};
    #var RouteName {};
    #var Route_list_short {};
    #loop @numitems{Route} 1 i {
        #var RouteName Route_@item{Route;$i}_@item{Route;@eval{$i-1}};
        #var Route_list_short ${$RouteName};
        #list Route_list add $Route_list_short[%*];
    };
    #nop //----------开始路径简化----------;
    #nop    #show $Route[%*];
    #nop    #show $Route_list[%*];
    #var i 1;
    #var j @numitems{Route_list};
    #while {$i<$j} {
        #var k @ismember{@item{Route_list;$i};Directions};
        #if {$k>0 & $k==@ismember{@item{Route_list;@eval{$i+1}};Directionsb}} {
            #list Route_list delete $i 2;
            #math j {$j-2};
            #if {$i>1} {#math i {$i-1}};
        };
        #else {
            #math i {$i+1};
        };
    };
    #nop    #show $Route_list[%*];
    #show {Start to walk};
};

#nop ==行走命令==

#act {^Start to walk} {
    #class walk open;
    #alias walk {
        #if {$step==$stepmax} {set brief 2};
        #if {$step<=$stepmax} {
            #var cmd @item{Route_list;$step};
            #if {"$cmd"=="stop"} {
                #math step {$step+1};
                #var stepaccu 1;
            };
            #else {
                #if {"$cmd"=="busy1"} {
                    #math step {$step+1};
                    #var stepaccu 1;
                    #delay {1} {walk};
                };
                #else {
                    $cmd;
                    #math stepaccu {$stepaccu+1};
                    #math step {$step+1};
                    #if {$stepaccu>=${xiaoyao.accu}} {
                        #delay {${xiaoyao.delay_long}} {walk};
                        #var stepaccu 1;
                    };
                    #else {
                        #delay {${xiaoyao.delay_short}} {walk};
                    };
                };
            };
        };
        #else {
            whisper;
        };
    };

    #act {^你要对谁耳语些什么？} {
        #var CurrentPosition $Destination;
        #class walk kill;
        rec;
        #var critical_running 0;
        #show 抵达目的地。;
        rec;
    };

    #alias guojiang {ask shao gong about jiang};
    #alias guohe {ask shao gong about huanghe};
    #alias gb_andao {enter shudong;say 天堂有路你不走呀;d;%%1;};
    #alias gy_yinzhe {s;w;w;e;s};
    #alias meizhuang_gate {knock gate 4;knock gate 2;knock gate 5;knock gate 3};
    #alias mr_mantuo {w;n;w;s;s;e;e;w;w};
    #alias mr_mantuob {e;s;n;w;n;n;e;s;e};
    #alias tiandihui_andao {knock guancai 3;ed;s;s;s;w;e;n;n;n;n;e};
    #alias tiandihui_andaob {w;#4 s;w;n;n;e;n;s;wu};
    #alias guohe3s2n {#if {"$char_menpai_zh"=="明教"} {sw;n;ban stone;you;#alias guohe3 {n;ne}} {guohe}};
    #alias guohe3n2s {#if {"$char_menpai_zh"=="明教"} {sw;s;ban stone;you;#alias guohe3 {s;ne}} {guohe}};
    #alias guohe3 {n;ne};
    #alias taohuadao {#if {"$char_menpai_zh"=="桃花岛"} {ride diao} {enter boat}};
    #alias baituofufei {#if {"$char_menpai_zh"=="白驼山"} {give qian 30 silver} {give qian 1 gold}};
    #alias murongboat {#if {"$char_menpai_zh"=="姑苏慕容"} {find boat} {enter boat}};
    #alias shenlongboat {#if {"$char_menpai_zh"=="神龙教"} {find boat} {enter boat}};
    #alias qytoyzw {#if {$mydodge>120} {e;cai yanziwu} {#3 n;tan qin;row yanziwu}};
    #alias qytotx {#if {$mydodge>120} {e;cai tingxiang} {#3 n;tan qin;row tingxiang}};
    #alias gotoqinyun {#if {$mydodge>120} {cai qinyun} {enter boat;row qinyun}};
    #alias gototingxiang {#if {$mydodge>120} {cai tingxiang} {enter boat;row tingxiang}};
    #alias gotoyanziwu {#if {$mydodge>120} {cai yanziwu} {enter boat;row yanziwu}};
    #act {^你沿着踏板走了上去。} {walk};
    #act {^你小心翼翼往前挪动，遇到艰险难行处，只好放慢脚步。} {#math step {$step-2}};
    #act {^青海湖畔美不胜收，你不由停下脚步，欣赏起了风景。} {#math step {$step-2}};
    #act {正等着你呢，上来吧。} {#class tmp kill;enter};
    #act {艄公将一块踏脚板搭上堤岸，} {#class tmp kill;enter};
    #act {^%*接过你递给的船资} {
        #class tmp open;
        #act {^只听得江面上隐隐传来：“别急嘛，这儿正忙着呐……”} {#class tmp kill;#delay {3} {yell boat}};
        #class tmp close;
    };
    #act {^你吸了口气，一声“船家”，声音中正平和地远远传了出去。$} {
        #class tmp open;
        #act {^只听得江面上隐隐传来：“别急嘛，这儿正忙着呐……”} {#class tmp kill;#delay {3} {yell boat}};
        #class tmp close;
    };
    #act {^你使出吃奶的力气喊了一声：“船家”$} {
        #class tmp open;
        #act {^只听得江面上隐隐传来：“别急嘛，这儿正忙着呐……”} {#class tmp kill;#delay {3} {yell boat}};
        #class tmp close;
    };
    #var in_boat 0;
    #act {^江心驶去。} {
        #var in_boat 1;
    };
    #act {^艄公说“到啦，上岸吧”，随即把一块踏脚板搭上堤岸。} {
        #if {${in_boat} == 1} {
            #var in_boat 0;
            #var stepaccu 1;out;walk            
        };
    };
    #act {^你反应迅速，急忙双手抱头，身体蜷曲。眼前昏天黑地，顺着山路直滚了下去。} {#class walk kill;#var CurrentPosition qbl};
    #act {^你从山上滚了下来，只觉得浑身无处不疼，还受了几处伤。} {#class walk kill;#var CurrentPosition dzf};
    #act {^不知过了多久，船终于靠岸了，你累得满头大汗。} {walk};
    #act {^小舟终于划到近岸，你从船上走了出来。} {walk};
    #act {^绿衣少女将小船系在树枝之上，你跨上岸去。} {walk};
    #act {^你定了定神，走了出来。} {walk};
    #act {^你身在半空，双手乱挥，只盼能抓到什么东西，这么乱挥一阵，又下堕下百馀丈。} {walk};
    #act {^你终于一步步的终于挨到了桥头} {walk};
    #act {^你朝船夫挥了挥手便跨上岸去。} {walk};
    #act {^你终于来到了对面，心里的石头终于落地。} {walk};
    #act {^%x道：你老人家快请上船。} {#delay {3} {yell boat}};
    #act {^半晌，你终于游到了对岸，随手扔掉了石块。} {guohe3;walk};
    #act {^你终于飞到了%*，白雕落了下来。} {walk};
    #act {^不一会儿，你来到参合庄。} {walk};
    #act {^不一会儿，小船划到了岸边。} {walk};
    #act {^不一会儿就到了神龙岛，你纵身跃上码头。} {walk};
    #act {^不一会儿就到了天津塘沽口，你走上码头。} {walk};
    #act {^你健步如飞，不一会就走到} {walk};
    #act {^六名雪山弟子一齐转动机关，吊桥便又升了起来。} {walk};
    #act {^艄公把踏脚板收了起来，竹篙一点，扁舟向江心驶去。} {yell boat};
    #act {^大车停稳了下来，你可以下车(xia)了。$} {walk};
    #act {^你要进入哪间房？$} {#send {sc};#delay 1 {enter $char_id}};
    #act {^%d%s《{广陵散|率意帖|溪山行旅图|呕血谱}》$} {#list sibao add {%%1}};
    #act {^柜子里目前存放的物品有：$} {
        #var sibao {};
        #delay {1} {
            #if {@numitems{sibao}==4} {
                #loop 4 1 i {getout @item{sibao;$i}};
                #delay 1 {walk};
            };
            #else {
                #show 你的柜子里四宝不齐或有多余，无法自动前往梅庄。;
                set brief 2;
            }
        };
    };

    #class walk close;

    #var step 1;
    #var stepmax @numitems{Route_list};
    #var stepaccu 1;
    set brief 1;
    walk;
};

